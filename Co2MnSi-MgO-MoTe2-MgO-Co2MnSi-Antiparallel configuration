# -------------------------------------------------------------
# Forces
# -------------------------------------------------------------
forces = Forces(device_configuration)
nlsave('Co2MnSi-MgO-MoTe2-MgO-Co2MnSi-anti.hdf5', forces)
nlprint(forces)

# -------------------------------------------------------------
# Mulliken Population
# -------------------------------------------------------------
mulliken_population = MullikenPopulation(device_configuration)
nlsave('Co2MnSi-MgO-MoTe2-MgO-Co2MnSi-anti.hdf5', mulliken_population)
nlprint(mulliken_population)

# -------------------------------------------------------------
# Transmission Spectrum
# -------------------------------------------------------------
kpoint_grid = MonkhorstPackGrid(
    na=51,
    nb=51,
    )

transmission_spectrum = TransmissionSpectrum(
    configuration=device_configuration,
    energies=numpy.linspace(-2, 2, 101)*eV,
    kpoints=kpoint_grid,
    energy_zero_parameter=AverageFermiLevel,
    infinitesimal=1e-06*eV,
    self_energy_calculator=RecursionSelfEnergy(),
    enforce_zero_in_band_gap=True,
    )
nlsave('Co2MnSi-MgO-MoTe2-MgO-Co2MnSi-anti.hdf5', transmission_spectrum)
nlprint(transmission_spectrum)
